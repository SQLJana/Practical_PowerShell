.sqlcode {
	font-size: 11px;
	font-weight: bold;
	font-family: monospace;
	white-space: pre;
    -o-tab-size: 4;
    -moz-tab-size: 4;
    -webkit-tab-size: 4;
}
.sqlcomment {
	color: #00aa00;
}
.sqlstring {
	color: #aa0000;
}
.sqlfunction {
	color: #aa00aa;
}
.sqlkeyword {
	color: #0000aa;
}
.sqloperator {
	color: #777777;
}